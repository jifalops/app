{
  "name": "App-Arch: API-Python",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "api-python",
  "remoteUser": "developer",
  "workspaceFolder": "/app-arch/api-python",
  "postCreateCommand": "../.devcontainer/api-python/post-create.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.black-formatter",
        "ms-python.isort",
        "Cameron.vscode-pytest",
        "EthanSK.restore-terminals",
        "tamasfe.even-better-toml",
        "redhat.vscode-yaml",
        "me-dutour-mathieu.vscode-github-actions",
        "mtxr.sqltools",
        "neo4j-extensions.neo4j-for-vscode"
      ]
    }
  }
}